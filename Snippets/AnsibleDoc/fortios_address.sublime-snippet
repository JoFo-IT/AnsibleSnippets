${6:   ${7:backup_filename}: ${8:"#"}}
${9:   ${10:backup_path}: ${11:"#"}}
${12:   ${13:comment}: ${14:"#"}}
${15:   ${16:config_file}: ${17:"#"}}
${18:   ${19:country}: ${20:"#"}}
${21:   ${22:end_ip}: ${23:"#"}}
${24:   ${25:file_mode}: ${26:"#"}}
${27:   ${28:host}: ${29:"#"}}
