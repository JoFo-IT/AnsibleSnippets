${39:   ${40:start_ip}: ${41:"#"}}
${42:   ${43:state}: ${44:"#"}}
${45:   ${46:timeout}: ${47:"#"}}
${48:   ${49:type}: ${50:"#"}}
${51:   ${52:username}: ${53:"#"}}
${54:   ${55:value}: ${56:"#"}}
${57:   ${58:vdom}: ${59:"#"}}
${60:${61:   become: ${62:true}} 
