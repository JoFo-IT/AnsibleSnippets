  elasticsearch_plugin: ${5:name=${6: } }${7:plugin_bin=${8: } }${9:plugin_dir=${10: } }${11:state=${12: } }${13:timeout=${14: } }${15:url=${16: } }${17:version=${18: } }
${19:  when: ${20: variable is defined}}
${21:  with_items: ${22: array}}
]]></content>
